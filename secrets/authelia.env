# --- Authelia Core Secrets ---
AUTHELIA_IDENTITY_VALIDATION_RESET_PASSWORD_JWT_SECRET=openssl rand -hex 32
AUTHELIA_SESSION_SECRET=openssl rand -hex 32
AUTHELIA_STORAGE_ENCRYPTION_KEY=openssl rand -hex 32

# --- OIDC Configuration ---
AUTHELIA_IDENTITY_PROVIDERS_OIDC_HMAC_SECRET=openssl rand -hex 32

# RSA Private Key for OIDC signing (JWKS)
# We will try to keep it as a single line or escaped for systemd
AUTHELIA_IDENTITY_PROVIDERS_OIDC_JWKS_0_KEY=openssl genrsa 2048

# Immich OIDC Client Secret
AUTHELIA_IDENTITY_PROVIDERS_OIDC_CLIENTS_0_CLIENT_SECRET=openssl rand -hex 32

# --- Google Upstream (Manual Entry) ---
AUTHELIA_IDENTITY_PROVIDERS_OIDC_UPSTREAM_PROVIDERS_0_CLIENT_ID=prompt("Enter Google OAuth Client ID: ")
AUTHELIA_IDENTITY_PROVIDERS_OIDC_UPSTREAM_PROVIDERS_0_CLIENT_SECRET=prompt("Enter Google OAuth Client Secret: ")
