# --- Authelia Core Secrets ---
# These are automatically generated using openssl
AUTHELIA_JWT_SECRET=openssl rand -hex 32
AUTHELIA_SESSION_SECRET=openssl rand -hex 32
AUTHELIA_STORAGE_ENCRYPTION_KEY=openssl rand -hex 32

# --- OIDC Configuration ---
AUTHELIA_IDENTITY_PROVIDERS_OIDC_HMAC_SECRET=openssl rand -hex 32
# RSA Private Key for OIDC signing
AUTHELIA_IDENTITY_PROVIDERS_OIDC_ISSUER_PRIVATE_KEY=openssl genrsa 4096
AUTHELIA_IDENTITY_PROVIDERS_OIDC_IMMICH_SECRET=openssl rand -hex 32

# --- Google Upstream (Manual Entry) ---
AUTHELIA_IDENTITY_PROVIDERS_OIDC_GOOGLE_CLIENT_ID=prompt("Enter Google OAuth Client ID (from Google Cloud Console): ")
AUTHELIA_IDENTITY_PROVIDERS_OIDC_GOOGLE_CLIENT_SECRET=prompt("Enter Google OAuth Client Secret: ")
